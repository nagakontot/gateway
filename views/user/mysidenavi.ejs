<style>
/******************************************************************/
/******************************************************************/
/* sidenavi wrapper */
#sideNavi,
.side-navi-item,
.side-navi-data,
.side-navi-tab 
{  margin:  0 auto; 
   padding: 0;  
}

#sideNavi 
{   position:    fixed;
    right:       50px;
    top:         20%;
    opacity:     0.618;
}
/* items */
.side-navi-item-default {
    height: 50px;
    display: none;
}
.side-navi-item {
    position: absolute;
    left: 0;
    display: inline-block;
    width: 95px;
    height: 40px;
    transform: rotate(-90deg);
    transform-origin: left top 0;
    color: #fff;
    background-color: blue;
    cursor: pointer;
}
.side-navi-item.item1 {  top: 100px;  }
.side-navi-item.item2 {  top: 200px;  }
.side-navi-item.item3 {  top: 300px;  }
.side-navi-item.item4 {  top: 400px;  }
.side-navi-item.item5 {  top: 500px;  }
.side-navi-item.item6 {  top: 600px;  }


.side-navi-item.active{  background-color: #bbb; color: #0ff;  }
.side-navi-item:hover {  background-color: #555; color: #ff0;  }
.side-navi-item > div {  padding-top: 15px; text-align: center;}

/* data wrapper */
.side-navi-data {
    position: absolute;
    top: 0;
    left: 45px;
    width: 320px;
    height: 600px;
    background-color: #bbb;
}
/* tab wrapper */
.side-navi-tab {  display: none;  }
.side-navi-tab.active {  display: inline-block;  }
.side-navi-tab > div {  padding: 30px;  }    

/******************************************************************/
/******************************************************************/
</style>

    <!-- start sidenavi wrapper//-->
    <div id="sideNavi">
        <!-- sidenavi menu //-->
        <div class="side-navi-item-default"></div>
        <div class="side-navi-item item1" title="this is item1"><div>Item 1</div></div>
        <div class="side-navi-item item2" title="this is item2"><div>Item 2</div></div>
        <div class="side-navi-item item3" title="this is item3"><div>Item 3</div></div>
        <div class="side-navi-item item4" title="this is item4"><div>Item 4</div></div>
        <div class="side-navi-item item5" title="this is item5"><div>Item 5</div></div>
        <div class="side-navi-item item6" title="this is item6"><div>Item 6</div></div>
        <!-- sidenavi data //-->
        <div class="side-navi-data">
            <div class="side-navi-tab"><div>Data Item 1</div></div>
            <div class="side-navi-tab"><div>Data Item 2</div></div>
            <div class="side-navi-tab"><div>Data Item 3</div></div>
            <div class="side-navi-tab"><div>Data Item 4</div></div>
            <div class="side-navi-tab"><div>Data Item 5</div></div>
            <div class="side-navi-tab"><!--div>Data Item 6</div-->
                <%include mymenu3D.ejs%>
            </div>
            
            
            
        </div>
    </div>
    <!-- end sidenavi wrapper//-->
    
<script src="js/lib/SideNavi/SideNavi.js"></script>
<script>
    $.smallipop.defaults.popupDelay = '10';
    //$.smallipop.defaults.theme      = 'orange';
        
    $('.item1').smallipop({cssAnimations: {enabled: true,show: 'animated bounceInDown',hide: 'animated hinge'}});
    $('.item2').smallipop({cssAnimations: {enabled: true,show: 'animated bounceInDown',hide: 'animated hinge'}});
    $('.item3').smallipop({cssAnimations: {enabled: true,show: 'animated bounceInDown',hide: 'animated hinge'}});
    $('.item4').smallipop({cssAnimations: {enabled: true,show: 'animated bounceInDown',hide: 'animated hinge'}});
    $('.item5').smallipop({cssAnimations: {enabled: true,show: 'animated bounceInDown',hide: 'animated hinge'}});
    $('.item6').smallipop({cssAnimations: {enabled: true,show: 'animated bounceInDown',hide: 'animated hinge'}});
    ////////////////////////////////////
    $(document).ready( function () 
    {   /**
         *  init sidenavi
         *  first param String direction left or right
         *  second param conf Object css data
         **/
        SideNavi.init('right', {container :     '#sideNavi',
                                defaultitem :   '.side-navi-item-default',
                                item :          '.side-navi-item',
                                data :          '.side-navi-data',
                                tab :           '.side-navi-tab',
                                active :        '.active'
                                });

    });
</script>
